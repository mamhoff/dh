<%- cache(element) do -%>

  <%# We act on the assumption that videos have an aspect ratio of 16 by 9 %>

  <article class="dh-section dh-vimeo-queen">
    <%= element_view_for(element,
      class: 'vimeo-queen-view embed-responsive embed-responsive-16by9') do |el| -%>
      <%= el.render :vimeo_code %>
    <%- end -%>
  </article>
<%- end -%>
