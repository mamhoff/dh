<%- cache(element) do -%>

  <%= element_view_for(element, tag: 'section',
                                id: 'dh-vimeo-gallery',
                                class: 'row') do |el| -%>

    <%= render(partial: '/shared/vimeo_gallery_element',
               collection: element.contents.where(name: ['vimeo_code'])) %>

  <%- end -%>
<%- end -%>
